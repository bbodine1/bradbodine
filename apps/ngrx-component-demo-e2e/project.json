{
  "name": "ngrx-component-demo-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ngrx-component-demo-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/ngrx-component-demo-e2e/cypress.config.ts",
        "devServerTarget": "ngrx-component-demo:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "ngrx-component-demo:serve:production"
        },
        "ci": {
          "devServerTarget": "ngrx-component-demo:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ngrx-component-demo-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["ngrx-component-demo"]
}
